<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath></OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Rg.Plugins.Popup" Version="2.0.0.2" />
    <PackageReference Include="SD.Tools.Algorithmia" Version="1.4.0" />
    <PackageReference Include="Xamarin.Forms" Version="4.5.0.617" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\FifthCharacter.Plugin\FifthCharacter.Plugin.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Attacks\Mechanics\Property\WeapPropName.Designer.cs">
      <DependentUpon>WeapPropName.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Attacks\Mechanics\Property\WeapPropTxt.Designer.cs">
      <DependentUpon>WeapPropTxt.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Features\PlayerClass\Fighter\FeatureFighterText.Designer.cs">
      <DependentUpon>FeatureFighterText.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Features\PlayerClass\Fighter\FightingStyleText.Designer.cs">
      <DependentUpon>FightingStyleText.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Popup\PopupFeature.xaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
    </Compile>
    <Compile Update="Popup\PopupFeature_GTK.xaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
    </Compile>
    <Compile Update="Spells\AtHigherLevelsDescriptions.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>AtHigherLevelsDescriptions.resx</DependentUpon>
    </Compile>
    <Compile Update="Spells\SpellDescriptions.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SpellDescriptions.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Attacks\Mechanics\Property\WeapPropName.resx">
      <LastGenOutput>WeapPropName.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Attacks\Mechanics\Property\WeapPropTxt.resx">
      <LastGenOutput>WeapPropTxt.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Features\PlayerClass\Fighter\FeatureFighterText.resx">
      <LastGenOutput>FeatureFighterText.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Features\PlayerClass\Fighter\FightingStyleText.resx">
      <LastGenOutput>FightingStyleText.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Spells\AtHigherLevelsDescriptions.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>AtHigherLevelsDescriptions.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Spells\SpellDescriptions.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>SpellDescriptions.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Proficiencies\Armor\" />
    <Folder Include="Proficiencies\Languages\" />
    <Folder Include="Proficiencies\Skills\" />
    <Folder Include="Proficiencies\Tools\" />
  </ItemGroup>

</Project>
